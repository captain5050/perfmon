      "Constants": [
        {
          "Name": "CHAS_PER_SOCKET",
          "Alias": "b"
        },
        {
          "Name": "SOCKET_COUNT",
          "Alias": "socket_count"
        }
      ],
      "Formula": "(a / (b * socket_count) / 1000000000) / DURATIONTIMEINSECONDS",
      "Category": "",
      "Threshold": "",
      "ResolutionLevels": "CHA, SOCKET, SYSTEM",
      "MetricGroup": ""
    },
